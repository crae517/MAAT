            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='C'
            handles.b2=3.26;
            handles.b3=6.24;
            handles.b4=5.55;
            handles.b5=54.54e10;
            handles.b6=45.13e10;
            handles.b7=1;
            handles.b8=2.1;
            handles.Aw2=12.011;
            handles.gam2=0;    %mN/m
            handles.b9=3823;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='C'
            handles.a2=3.26;
            handles.a3=6.24;
            handles.a4=5.55;
            handles.a5=54.54e10;
            handles.a6=45.13e10;
