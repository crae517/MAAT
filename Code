            handles.Aw3=118.71;
            handles.gam3=560;       %mN/m
            handles.d9=505.08;     %K
            set(handles.t6,'String',handles.d1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='Sn'
            handles.b2=16.3;
            handles.b3=4.15;
            handles.b4=1.9;
            handles.b5=11.09e10;
            handles.b6=1.844e10;
            handles.b7=1;
            handles.b8=2.1;
            handles.Aw2=118.71;
            handles.gam2=560;    %mN/m
            handles.b9=505.08;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='Sn'
            handles.a2=16.3;
            handles.a3=4.15;
            handles.a4=1.9;
            handles.a5=11.09e10;
            handles.a6=1.844e10;
            handles.a7=1;
            handles.a8=2.1;
            handles.Aw1=118.71;
            handles.gam1=560;    %mN/m
            handles.a9=505.08;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;       
        elseif handles.contador==4
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unpreslitio);
        if handles.a1=='Sn'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t4,'String',handles.b1)
            set(handles.t5,'String',handles.d1)
 
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Sn'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        else
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
end
handles
guidata(hObject, handles);

% --- Executes on button press in Pb.
function Pb_Callback(hObject, eventdata, handles)
% hObject    handle to Pb (see GCBO)
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
die=get(hObject,'Value');
handles.die=die;
if handles.sis==1
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.preslitio);
        if handles.contador==2
            set(handles.Pb,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unpreslitio);
        elseif handles.contador==1
            handles.b1='Pb'
            handles.b2=18.28;
            handles.b3=4.1;
            handles.b4=1.52;
            handles.b5=4.299e10;
            handles.b6=0.5396e10;
            handles.b7=1;
            handles.b8=2.1;
            handles.Aw2=207.2;
            handles.gam2=458;    %mN/m
            handles.b9=600.61;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==0
            handles.a1='Pb'
            handles.a2=18.28;
            handles.a3=4.1;
            handles.a4=1.52;
            handles.a5=4.299e10;
            handles.a6=0.5396e10;
            handles.a7=1;        
            handles.a8=2.1;
            handles.Aw1=207.2;
            handles.gam1=458;
            handles.a9=600.61;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;
        
        elseif handles.contador==3
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unpreslitio);
        if handles.a1=='Pb'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;     %K
            set(handles.t4,'String',handles.b1)
        
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Pb'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.d1=='Pb'
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
else
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.preslitio);
        if handles.contador==3
            set(handles.Pb,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unpreslitio);
        elseif handles.contador==2
            handles.d1='Pb'
            handles.d2=18.28;
            handles.d3=4.1;
            handles.d4=1.52;
            handles.d5=4.299e10;
            handles.d6=0.5396e10;
            handles.d7=1;
            handles.d8=2.1;
            handles.Aw3=207.2;
            handles.gam3=458;       %mN/m
            handles.d9=600.61;     %K
            set(handles.t6,'String',handles.d1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='Pb'
            handles.b2=18.28;
            handles.b3=4.1;
            handles.b4=1.52;
            handles.b5=4.299e10;
            handles.b6=0.5396e10;
            handles.b7=1;
            handles.b8=2.1;
            handles.Aw2=207.2;
            handles.gam2=458;    %mN/m
            handles.b9=600.61;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='Pb'
            handles.a2=18.28;
            handles.a3=4.1;
            handles.a4=1.52;
            handles.a5=4.299e10;
            handles.a6=0.5396e10;
            handles.a7=1;
            handles.a8=2.1;
            handles.Aw1=207.2;
            handles.gam1=458;    %mN/m
            handles.a9=600.61;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;       
        elseif handles.contador==4
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unpreslitio);
        if handles.a1=='Pb'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t4,'String',handles.b1)
            set(handles.t5,'String',handles.d1)
 
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Pb'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        else
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
end
