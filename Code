            handles.d9=317.3;     %K
            set(handles.t6,'String',handles.d1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='P'
            handles.b2=8.6;
            handles.b3=5.55;
            handles.b4=4.49;
            handles.b5=1.919e10;
            handles.b6=0.7161e10;
            handles.b7=1;
            handles.b8=2.3;
            handles.Aw2=30.973762;
            handles.gam2=52;    %mN/m
            handles.b9=317.3;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='P'
            handles.a2=8.6;
            handles.a3=5.55;
            handles.a4=4.49;
            handles.a5=1.919e10;
