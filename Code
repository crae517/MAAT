            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unprescalcio);
        if handles.a1=='Rh'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;
            set(handles.t4,'String',handles.b1)
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
 
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Rh'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        else
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
end
handles
guidata(hObject, handles);

% --- Executes on button press in Ir.
function Ir_Callback(hObject, eventdata, handles)
% hObject    handle to Ir (see GCBO)
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
die=get(hObject,'Value');
handles.die=die;
if handles.sis==1
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.prescalcio);
        if handles.contador==2
            set(handles.Ir,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unprescalcio);
        elseif handles.contador==1
            handles.b1='Ir'
            handles.b2=8.52;
            handles.b3=5.55;
            handles.b4=6.13;
            handles.b5=35.51e10;
            handles.b6=20.99e10;
            handles.b7=0;
            handles.b8=1;
            handles.Aw2=192.22;
            handles.gam2=2250;       %mN/m
            handles.b9=2739;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==0
            handles.a1='Ir'
            handles.a2=8.52;
            handles.a3=5.55;
            handles.a4=6.13;
            handles.a5=35.51e10;
            handles.a6=20.99e10;
            handles.a7=0;
            handles.a8=1;
            handles.Aw1=192.22;
            handles.gam1=2250;    %mN/m
            handles.a9=2739;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;
        
        elseif handles.contador==3
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unprescalcio);
        if handles.a1=='Ir'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;     %K
            set(handles.t4,'String',handles.b1)
 
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Ir'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.d1=='Ir'
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
else
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.prescalcio);
        if handles.contador==3
            set(handles.Ir,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unprescalcio);
        elseif handles.contador==2
            handles.d1='Ir'
            handles.d2=8.52;
            handles.d3=5.55;
            handles.d4=6.13;
            handles.d5=35.51e10;
            handles.d6=20.99e10;
            handles.d7=0;
            handles.d8=1;
            handles.Aw3=192.22;
            handles.gam3=2250;       %mN/m
            handles.d9=2739;     %K
            set(handles.t6,'String',handles.d1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='Ir'
            handles.b2=8.52;
            handles.b3=5.55;
            handles.b4=6.13;
            handles.b5=35.51e10;
            handles.b6=20.99e10;
            handles.b7=0;
            handles.b8=1;
            handles.Aw2=192.22;
            handles.gam2=2250;    %mN/m
            handles.b9=2739;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='Ir'
            handles.a2=8.52;
            handles.a3=5.55;
            handles.a4=6.13;
            handles.a5=35.51e10;
            handles.a6=20.99e10;
            handles.a7=0;
            handles.a8=1;
            handles.Aw1=192.22;
            handles.gam1=2250;    %mN/m
            handles.a9=2739;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;       
        elseif handles.contador==4
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unprescalcio);
        if handles.a1=='Ir'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;
            set(handles.t4,'String',handles.b1)
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
 
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Ir'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        else
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
end
handles
guidata(hObject, handles);

% --- Executes on button press in Ni.
function Ni_Callback(hObject, eventdata, handles)
% hObject    handle to Ni (see GCBO)
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
die=get(hObject,'Value');
handles.die=die;
if handles.sis==1
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.prescalcio);
        if handles.contador==2
            set(handles.Ni,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unprescalcio);
        elseif handles.contador==1
            handles.b1='Ni'
            handles.b2=6.6;
            handles.b3=5.2;
            handles.b4=5.36;
            handles.b5=18.64e10;
            handles.b6=7.505e10;
            handles.b7=0;
            handles.b8=1;
            handles.Aw2=58.69;
            handles.gam2=1778;       %mN/m
            handles.b9=1728;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==0
            handles.a1='Ni'
            handles.a2=6.6;
            handles.a3=5.2;
            handles.a4=5.36;
            handles.a5=18.64e10;
            handles.a6=7.505e10;
            handles.a7=0;
            handles.a8=1;
            handles.Aw1=58.69;
            handles.gam1=1778;    %mN/m
            handles.a9=1728;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;
        
        elseif handles.contador==3
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unprescalcio);
        if handles.a1=='Ni'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;     %K
            set(handles.t4,'String',handles.b1)
 
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Ni'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.d1=='Ni'
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
