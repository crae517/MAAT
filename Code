            handles.b1='Hg'
            handles.b2=14.08;
            handles.b3=4.2;
            handles.b4=1.91;
            handles.b5=2.825e10;
            handles.b6=1.001e10;
            handles.b7=1;
            handles.b8=1.4;
            handles.Aw2=200.59;
            handles.gam2=498;    %mN/m
            handles.b9=234.32;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==0
            handles.a1='Hg'
            handles.a2=14.08;
            handles.a3=4.2;
            handles.a4=1.91;
            handles.a5=2.825e10;
            handles.a6=1.001e10;
            handles.a7=1;        
