% --- Executes on button press in Rh.
function Rh_Callback(hObject, eventdata, handles)
% hObject    handle to Rh (see GCBO)
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
die=get(hObject,'Value');
handles.die=die;
if handles.sis==1
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.prescalcio);
        if handles.contador==2
            set(handles.Rh,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unprescalcio);
        elseif handles.contador==1
            handles.b1='Rh'
            handles.b2=8.3;
            handles.b3=5.4;
            handles.b4=5.45;
            handles.b5=27.06e10;
            handles.b6=14.72e10;
            handles.b7=0;
            handles.b8=1;
            handles.Aw2=102.90550;
            handles.gam2=2000;       %mN/m
            handles.b9=2237;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==0
            handles.a1='Rh'
            handles.a2=8.3;
            handles.a3=5.4;
            handles.a4=5.45;
            handles.a5=27.06e10;
            handles.a6=14.72e10;
