            set(hObject,'Backgroundcolor', handles.unprescalcio);
        elseif handles.contador==1
            handles.b1='Mn'
            handles.b2=7.35;
            handles.b3=4.45;
            handles.b4=4.17;
            handles.b5=5.967e10;
            handles.b6=7.652e10;
            handles.b7=0;
            handles.b8=1;
            handles.Aw2=54.93805;
            handles.gam2=1090;       %mN/m
            handles.b9=1519;     %K
