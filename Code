% --- Executes on button press in Ti.
function Ti_Callback(hObject, eventdata, handles)
% hObject    handle to Ti (see GCBO)
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
die=get(hObject,'Value');
handles.die=die;
if handles.sis==1
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.prescalcio);
        if handles.contador==2
            set(handles.Ti,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unprescalcio);
        elseif handles.contador==1
            handles.b1='Ti'
            handles.b2=10.58;
            handles.b3=3.8;
            handles.b4=3.51;
            handles.b5=10.52e10;
            handles.b6=3.934e10;
            handles.b7=0;
            handles.b8=1;
            handles.Aw2=47.88;
            handles.gam2=1650;       %mN/m
            handles.b9=1941;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==0
            handles.a1='Ti'
            handles.a2=10.58;
            handles.a3=3.8;
            handles.a4=3.51;
            handles.a5=10.52e10;
            handles.a6=3.934e10;
            handles.a7=0;
            handles.a8=1;
            handles.Aw1=47.88;
            handles.gam1=1650;    %mN/m
            handles.a9=1941;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;
        
        elseif handles.contador==3
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unprescalcio);
        if handles.a1=='Ti'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;     %K
            set(handles.t4,'String',handles.b1)
 
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Ti'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.d1=='Ti'
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
else
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.prescalcio);
        if handles.contador==3
            set(handles.Ti,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unprescalcio);
        elseif handles.contador==2
            handles.d1='Ti'
            handles.d2=10.58;
            handles.d3=3.8;
            handles.d4=3.51;
            handles.d5=10.52e10;
            handles.d6=3.934e10;
            handles.d7=0;
            handles.d8=1;
            handles.Aw3=47.88;
            handles.gam3=1650;       %mN/m
            handles.d9=1941;     %K
            set(handles.t6,'String',handles.d1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='Ti'
            handles.b2=10.58;
            handles.b3=3.8;
            handles.b4=3.51;
            handles.b5=10.52e10;
            handles.b6=3.934e10;
            handles.b7=0;
            handles.b8=1;
            handles.Aw2=47.88;
            handles.gam2=1650;    %mN/m
            handles.b9=1941;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='Ti'
            handles.a2=10.58;
            handles.a3=3.8;
            handles.a4=3.51;
            handles.a5=10.52e10;
            handles.a6=3.934e10;
            handles.a7=0;
            handles.a8=1;
            handles.Aw1=47.88;
            handles.gam1=1650;    %mN/m
            handles.a9=1941;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;       
        elseif handles.contador==4
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unprescalcio);
        if handles.a1=='Ti'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;
            set(handles.t4,'String',handles.b1)
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
 
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Ti'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        else
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
end
