            handles.b1='Zn'
            handles.b2=9.17;
            handles.b3=4.1;
            handles.b4=2.3;
            handles.b5=5.985e10;
            handles.b6=3.718e10;
            handles.b7=1;
            handles.b8=1.4;
            handles.Aw2=65.39;
            handles.gam2=782;    %mN/m
            handles.b9=692.68;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='Zn'
            handles.a2=9.17;
            handles.a3=4.1;
            handles.a4=2.3;
