            set(handles.t4,'String',handles.b1)
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
 
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Ba'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        else
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
end
handles
guidata(hObject, handles);

% --- Executes on button press in Sc.
function Sc_Callback(hObject, eventdata, handles)
% hObject    handle to Sc (see GCBO)
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
die=get(hObject,'Value');
handles.die=die;
if handles.sis==1
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.prescalcio);
        if handles.contador==2
            set(handles.Sc,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unprescalcio);
        elseif handles.contador==1
            handles.b1='Sc'
            handles.b2=15.03;
            handles.b3=3.25;
            handles.b4=2.05;
            handles.b5=5.7e10;
            handles.b6=2.9e10;
            handles.b7=0;
            handles.b8=0.7;
            handles.Aw2=44.955910;
            handles.gam2=0;       %mN/m
            handles.b9=1814;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==0
            handles.a1='Sc'
            handles.a2=15.03;
            handles.a3=3.25;
            handles.a4=2.05;
            handles.a5=5.7e10;
            handles.a6=2.9e10;
            handles.a7=0;
            handles.a8=0.7;
            handles.Aw1=44.955910;
            handles.gam1=0;    %mN/m
            handles.a9=1814;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;
        
        elseif handles.contador==3
            set(handles.Sc,'Value',0)
            set(hObject,'Backgroundcolor', handles.unprescalcio);
            handles.contador=2
        end
    else
        set(hObject,'Backgroundcolor', handles.unprescalcio);
        if handles.a1=='Sc'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;     %K
            set(handles.t4,'String',handles.b1)
 
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Sc'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.d1=='Sc'
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
else
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.prescalcio);
        if handles.contador==3
            set(handles.Sc,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unprescalcio);
        elseif handles.contador==2
            handles.d1='Sc'
            handles.d2=15.03;
            handles.d3=3.25;
            handles.d4=2.05;
            handles.d5=5.7e10;
            handles.d6=2.9e10;
            handles.d7=0;
            handles.d8=0.7;
            handles.Aw3=44.955910;
            handles.gam3=0;       %mN/m
            handles.d9=1814;     %K
            set(handles.t6,'String',handles.d1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='Sc'
            handles.b2=15.03;
            handles.b3=3.25;
            handles.b4=2.05;
            handles.b5=5.7e10;
            handles.b6=2.9e10;
            handles.b7=0;
            handles.b8=0.7;
            handles.Aw2=44.955910;
            handles.gam2=0;    %mN/m
            handles.b9=1814;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='Sc'
            handles.a2=15.03;
            handles.a3=3.25;
            handles.a4=2.05;
            handles.a5=5.7e10;
            handles.a6=2.9e10;
            handles.a7=0;
            handles.a8=0.7;
            handles.Aw1=44.955910;
            handles.gam1=0;    %mN/m
            handles.a9=1814;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;       
        elseif handles.contador==4
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unprescalcio);
        if handles.a1=='Sc'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;
            set(handles.t4,'String',handles.b1)
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
 
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Sc'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        else
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
