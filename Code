% --- Executes on button press in P.
function Sb_Callback(hObject, eventdata, handles)
% hObject    handle to Sb (see GCBO)
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
die=get(hObject,'Value');
handles.die=die;
if handles.sis==1
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.preslitio);
        if handles.contador==2
            set(handles.Sb,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unpreslitio);
        elseif handles.contador==1
            handles.b1='Sb'
            handles.b2=16.95;
            handles.b3=4.40;
            handles.b4=2.00;
            handles.b5=3.829e10;
            handles.b6=2.001e10;
            handles.b7=1;
            handles.b8=2.3;
            handles.Aw2=121.76;
            handles.gam2=0;    %mN/m
            handles.b9=903.78;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==0
            handles.a2=16.95;
            handles.a3=4.40;
            handles.a4=2.00;
            handles.a5=3.829e10;
            handles.a6=2.001e10;
            handles.a7=1;
            handles.a8=2.3;
            handles.Aw1=121.76;
            handles.gam1=0;    %mN/m
            handles.a9=903.78;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;
        
        elseif handles.contador==3
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unpreslitio);
        if handles.a1=='Sb'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;     %K
            set(handles.t4,'String',handles.b1)
        
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Sb'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.d1=='Sb'
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
else
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.preslitio);
        if handles.contador==3
            set(handles.As,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unpreslitio);
        elseif handles.contador==2
            handles.d1='Sb'
            handles.d2=16.95;
            handles.d3=4.40;
            handles.d4=2.00;
            handles.d5=3.829e10;
            handles.d6=2.001e10;
            handles.d7=1;
            handles.d8=2.3;
            handles.Aw3=121.76;
            handles.gam3=0;    %mN/m
            handles.d9=903.78;     %K
            set(handles.t6,'String',handles.d1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='Sb'
            handles.b2=16.95;
            handles.b3=4.40;
            handles.b4=2.00;
            handles.b5=3.829e10;
            handles.b6=2.001e10;
            handles.b7=1;
            handles.b8=2.3;
            handles.Aw2=121.76;
            handles.gam2=0;    %mN/m
            handles.b9=903.78;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='Sb'
            handles.a2=16.95;
            handles.a3=4.40;
            handles.a4=2.00;
            handles.a5=3.829e10;
            handles.a6=2.001e10;
            handles.a7=1;
            handles.a8=2.3;
            handles.Aw1=121.76;
            handles.gam1=0;    %mN/m
            handles.a9=903.78;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;       
        elseif handles.contador==4
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unpreslitio);
        if handles.a1=='Sb'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t4,'String',handles.b1)
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Sb'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        else
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
end
handles
guidata(hObject, handles);

% --- Executes on button press in Bi.
function Bi_Callback(hObject, eventdata, handles)
% hObject    handle to Bi (see GCBO)
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
die=get(hObject,'Value');
handles.die=die;
if handles.sis==1
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.preslitio);
        if handles.contador==2
            set(handles.Bi,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unpreslitio);
        elseif handles.contador==1
            handles.b1='Bi'
            handles.b2=19.32;
            handles.b3=4.14;
            handles.b4=1.56;
            handles.b5=3.148e10;
            handles.b6=1.285e10;
            handles.b7=1;
            handles.b8=2.3;
            handles.Aw2=208.9804;
            handles.gam2=378;    %mN/m
            handles.b9=544.44;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==0
            handles.a1='Bi'
            handles.a2=19.32;
            handles.a3=4.14;
            handles.a4=1.56;
            handles.a5=3.148e10;
            handles.a6=1.285e10;
            handles.a7=1;        
            handles.a8=2.3;
            handles.Aw1=208.9804;
            handles.gam1=378;
            handles.a9=544.44;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;
        
        elseif handles.contador==3
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unpreslitio);
        if handles.a1=='Bi'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;     %K
            set(handles.t4,'String',handles.b1)
        
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Bi'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.d1=='Bi'
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
else
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.preslitio);
        if handles.contador==3
            set(handles.Bi,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unpreslitio);
        elseif handles.contador==2
            handles.d1='Bi'
            handles.d2=19.32;
            handles.d3=4.14;
            handles.d4=1.56;
            handles.d5=3.148e10;
            handles.d6=1.285e10;
            handles.d7=1;
            handles.d8=2.3;
            handles.Aw3=208.9804;
            handles.gam3=378;       %mN/m
            handles.d9=544.44;     %K
            set(handles.t6,'String',handles.d1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='Bi'
            handles.b2=19.32;
            handles.b3=4.14;
            handles.b4=1.56;
            handles.b5=3.148e10;
            handles.b6=1.285e10;
            handles.b7=1;
            handles.b8=2.3;
            handles.Aw2=208.9804;
            handles.gam2=378;    %mN/m
            handles.b9=544.44;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='Bi'
            handles.a2=19.32;
            handles.a3=4.14;
            handles.a4=1.56;
            handles.a5=3.148e10;
            handles.a6=1.285e10;
            handles.a7=1;
            handles.a8=2.3;
            handles.Aw1=208.9804;
            handles.gam1=378;    %mN/m
            handles.a9=544.44;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;       
        elseif handles.contador==4
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unpreslitio);
        if handles.a1=='Bi'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t4,'String',handles.b1)
            set(handles.t5,'String',handles.d1)
 
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Bi'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        else
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
end
handles
