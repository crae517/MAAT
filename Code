            handles.d9=2128;     %K
            set(handles.t6,'String',handles.d1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='Zr'
            handles.b2=14;
            handles.b3=3.45;
            handles.b4=2.8;
            handles.b5=8.335e10;
            handles.b6=3.414e10;
            handles.b7=0;
            handles.b8=1;
            handles.Aw2=91.224;
            handles.gam2=1480;    %mN/m
            handles.b9=2128;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='Zr'
            handles.a2=14;
            handles.a3=3.45;
            handles.a4=2.8;
            handles.a5=8.335e10;
            handles.a6=3.414e10;
