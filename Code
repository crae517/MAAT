            set(handles.Sn,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unpreslitio);
        elseif handles.contador==1
            handles.b1='Sn'
            handles.b2=16.3;
            handles.b3=4.15;
            handles.b4=1.9;
            handles.b5=11.09e10;
            handles.b6=1.844e10;
            handles.b7=1;
            handles.b8=2.1;
            handles.Aw2=118.71;
            handles.gam2=560;    %mN/m
            handles.b9=505.08;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==0
            handles.a1='Sn'
            handles.a2=16.3;
