            handles.d8=1;
            handles.Aw3=58.69;
            handles.gam3=1778;       %mN/m
            handles.d9=1728;     %K
            set(handles.t6,'String',handles.d1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='Ni'
            handles.b2=6.6;
            handles.b3=5.2;
            handles.b4=5.36;
            handles.b5=18.64e10;
            handles.b6=7.505e10;
            handles.b7=0;
            handles.b8=1;
            handles.Aw2=58.69;
            handles.gam2=1778;    %mN/m
            handles.b9=1728;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='Ni'
            handles.a2=6.6;
            handles.a3=5.2;
            handles.a4=5.36;
            handles.a5=18.64e10;
            handles.a6=7.505e10;
            handles.a7=0;
            handles.a8=1;
            handles.Aw1=58.69;
            handles.gam1=1778;    %mN/m
            handles.a9=1728;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;       
