function varargout = Miedema_v3(varargin)
% Miedema_v3 MATLAB code for Miedema_v3.fig
%      Miedema_v3, by itself, creates t12 new Miedema_v3 or raises the existing
%      singleton*.
%
%      H = Miedema_v3 returns the handle to t12 new Miedema_v3 or the handle to
%      the exist ing singleton*.
%
%      Miedema_v3('CALLBACK',hObject,eventData,handles,...) calls the local
%      function named CALLBACK in Miedema_v3.M with the given input arguments.
%
%      Miedema_v3('Property','Value',...) creates t12 new Miedema_v3 or raises the
%      existing singleton*.  Starting from the left, property value pairs are
%      applied to the GUI before Miedema_v3_OpeningFcn gets called.  An
%      unrecognized property name or invalid value makes property application
%      stop.  All inputs are passed to Miedema_v3_OpeningFcn via varargin.
%
%      *See GUI Options on GUIDE's Tools menu.  Choose "GUI allows only one
%      instance to run (singleton)".
%
% See also: GUIDE, GUIDATA, GUIHANDLES

% % Edit the above text to modify the response to help Miedema_v3

% Last Modified by GUIDE v2.5 03-Mar-2020 07:03:40

% Begin initialization code - DO NOT EDIT
gui_Singleton = 1;
gui_State = struct('gui_Name',       mfilename, ...
                   'gui_Singleton',  gui_Singleton, ...
                   'gui_OpeningFcn', @Miedema_v3_OpeningFcn, ...
                   'gui_OutputFcn',  @Miedema_v3_OutputFcn, ...
                   'gui_LayoutFcn',  [] , ...
                   'gui_Callback',   []);
if nargin && ischar(varargin{1})
    gui_State.gui_Callback = str2func(varargin{1});
end

if nargout
    [varargout{1:nargout}] = gui_mainfcn(gui_State, varargin{:});
else
    gui_mainfcn(gui_State, varargin{:});
end

% End initialization code - DO NOT EDIT
  

% --- Executes just before Miedema_v3 is made visible.
function Miedema_v3_OpeningFcn(hObject, eventdata, handles, varargin)
% This function has no output args, see OutputFcn.
% hObject    handle to figure
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
% varargin   command line arguments to Miedema_v3 (see VARARGIN)
handles.preslitio= [0.7 0.5 1];
handles.unpreslitio= [0.4 0.6 1];
handles.prescalcio= [1 0.8 0];
handles.unprescalcio = [ 1 1 0];
handles.a1=0;
handles.a2=0;
handles.a3=0;
handles.a4=0;
handles.a5=0;
handles.a6=0;
handles.a7=0;
handles.a8=0;
handles.a9=0;
handles.b1=0;
handles.b2=0;
handles.b3=0;
handles.b4=0;
handles.b5=0;
handles.b6=0;
handles.b7=0;
handles.b8=0;
handles.b9=0;
handles.d1=0;
handles.d2=0;
handles.d3=0;
handles.d4=0;
handles.d5=0;
handles.d6=0;
handles.d7=0;
handles.d8=0;
handles.d9=0;
handles.contador=0;
handles.T=298;
handles.x=0;
handles.v12=0;
handles.v13=0;
handles.v14=0;
handles.v1=0;
handles.v2=0;
handles.v6=100000;
handles.v7=6/handles.v6;
handles.Aw1=0;
handles.Aw2=0;
handles.Aw3=0;
handles.gam1=0;
handles.gam2=0;
handles.gam3=0;
handles.v9=0;
handles.v8=0;
handles.v10=handles.v9;
handles.v5=0;
handles.v4=0;
handles.v3=0;
handles.v11=0;
handles.mode=1; 
handles.sis=1;
handles.graph=3;
% Choose default command line output for Miedema_v3
handles.output = hObject;
handles.x=[0,0.1,0.2];
handles.y=[0,0.1,0.2];
handles.z=[1,0.8,0.6];
handles.v12=[0,0,0];
% Update handles structure
guidata(hObject, handles);

% UIWAIT makes Miedema_v3 wait for user response (see UIRESUME)
% uiwait(handles.figure1);


% --- Outputs from this function are returned to the command line.
function varargout = Miedema_v3_OutputFcn(hObject, eventdata, handles) 
% varargout  cell array for returning output args (see VARARGOUT);
% hObject    handle to figure
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
% Get default command line output from handles structure
varargout{1} = handles.output;

