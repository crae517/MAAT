% --- Executes on button press in c10.
function c10_Callback(hObject, eventdata, handles)
% hObject    handle to c10 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
handles.dislocaciones=get(hObject,'Value')
% Hint: get(hObject,'Value') returns toggle state of c8
if handles.dislocaciones==1
    set(handles.e7,'enable','on');
    handles.v11=str2num(get(handles.e7,'String'));
    set(handles.e8,'enable','on');
    handles.v3=str2num(get(handles.e8,'String'));
    set(handles.e9,'enable','on');
    handles.v4=str2num(get(handles.e9,'String'));
    set(handles.e10,'enable','on');
    handles.v5=str2num(get(handles.e10,'String'));
else
    set(handles.e7,'enable','off');
    e7_Callback(hObject, eventdata, handles);
    handles.v11=0;
    set(handles.e8,'enable','off');
    e8_Callback(hObject, eventdata, handles);
    handles.v3=0;
    set(handles.e10,'enable','off');
    e10_Callback(hObject, eventdata, handles);
    handles.v5=0;
    set(handles.e9,'enable','off');
    e9_Callback(hObject, eventdata, handles);
    handles.v4=0;

end
guidata(hObject,handles)
% Hint: get(hObject,'Value') returns toggle state of c10

