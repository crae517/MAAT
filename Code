% --------------------------------------------------------------------
function abrir_Callback(hObject, eventdata, handles)
% hObject    handle to abrir (see GCBO)
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
figure;

% --------------------------------------------------------------------
function save_Callback(hObject, eventdata, handles)
% hObject    handle to save (see GCBO)
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
mod=get(handles.r5,'Value');
if mod==1
    bba=get(handles.t10,'String');
    bbb=get(handles.text39,'String');
    bbc=get(handles.text40,'String');
    bbe=get(handles.text42,'String');
    Savtext={bba;bbb;bbc;bbd;bbe};
    Nomtex={'';'Gibbs Free Energy [kJ/mol]:   ';'Chemical Enthalpy [kJ/mol]:   ';'Elastic Enthalpy [kJ/mol]:    ';'T*Entropy [kJ/mol]:           '};
    Resltex=strcat(Nomtex,Savtext);
    [name,path] = uiputfile('*.txt');
            namev = fullfile(path,name);
            fid = fopen(namev,'w');
            fprintf(fid,'%s\r\n',Resltex{1:5});
            fclose(fid);
else
    if handles.sis==2
        if handles.graph==1
            figure;
            tersurfneg(handles.x,handles.y,handles.z,handles.m41);
            ternlabel(handles.abc,handles.bca,handles.cba);
        elseif handles.graph==2
            figure;
            tersurfneg(handles.x,handles.y,handles.z,handles.m39);
            ternlabel(handles.abc,handles.bca,handles.cba);
        elseif handles.graph==3
            figure;
            tersurfneg(handles.x,handles.y,handles.z,handles.v14);
            ternlabel(handles.abc,handles.bca,handles.cba);
        elseif handles.graph==6
            figure;
            tersurfneg(handles.x,handles.y,handles.z,-handles.v14);
            ternlabel(handles.abc,handles.bca,handles.cba);
        else
            figure;
            tersurfneg(handles.x,handles.y,handles.z,handles.v12);
            ternlabel(handles.abc,handles.bca,handles.cba);
        end 
    else
        oldAx=get(handles.g2,'children');
        fig=figure; 
        ax=axes; 
        copyobj(oldAx,ax)
        box on  
        ylabel('(kJ/mol)');
        Alab=get(handles.t12,'String');
        Blab=get(handles.t13,'String');
        xlabel(strcat(Alab,{'                                                                                                '},Blab));
        gg1=get(handles.c1,'Value');
        gg3=get(handles.c5,'Value');
        gg2=get(handles.c6,'Value');
        gg4=get(handles.c4,'Value');
        gg5=get(handles.c2,'Value');
        gg6=get(handles.c3,'Value');
        if gg1==1
            aaa='\Delta Gmod';
        else
            aaa='';
        end

        if gg2==1
            aab='\Delta Hc';
        else
            aab=zeros;
        end

        if gg3==1
            aac='\Delta Hel';
        else
            aac=zeros;
        end

        if gg4==1 
            aad='\Delta S';
        else
            aad=zeros;
        end

        if gg5==1
            aae='\Delta Gmix';
        else
            aae=zeros;
        end

        if gg6==1
            aaf='\Delta Hmix';
        else
            aaf=zeros;
        end
        legend(aaa,aae,aaf,aad,aac,aab);
    end
end


% --- Executes when selected object is changed in sistem.
function sistem_SelectionChangedFcn(hObject, eventdata, handles)
% hObject    handle to the selected object in sistem 
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
sis=get(hObject,'String');
switch sis
    case'Binary'
        handles.sis=1;
    case'Ternary'
        handles.sis=2;
end
handles;
guidata(hObject, handles);

% --- Executes when selected object is changed in mod.
function mod_SelectionChangedFcn(hObject, eventdata, handles)
% hObject    handle to the selected object in mod 
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
mode=get(hObject,'String');
switch mode
    case'Amorphous'
        handles.mode=2;
    case'Solid solution'
        handles.mode=1;
    case'Intermetalic'
        handles.mode=3;
end
handles;
guidata(hObject, handles);

% --- Executes on button press in r3.
function r3_Callback(hObject, eventdata, handles)
% hObject    handle to r3 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)

% Hint: get(hObject,'Value') returns toggle state of r3
set(handles.c8,'enable','off')
set(handles.c9,'enable','off')
set(handles.c10,'enable','off')
set(handles.t12,'visible','on')
set(handles.t13,'visible','on')
set(handles.t5,'visible','off')
set(handles.t4,'visible','off')
set(handles.t1,'visible','off')
set(handles.t2,'visible','off')
set(handles.t7,'visible','off')
set(handles.t8,'visible','off')
set(handles.e11,'visible','off')
set(handles.e12,'visible','off')
set(handles.t10,'visible','off')
set(handles.t11,'visible','off')
set(handles.text39,'visible','off')
set(handles.text40,'visible','off')
set(handles.text42,'visible','off')
set(handles.c1,'Value',0);
set(handles.c1,'enable','off');
set(handles.c6,'enable','on');
set(handles.c5,'enable','on');
set(handles.c5,'String','Topological enthalpy(Black dashed)');
set(handles.c4,'enable','on');
set(handles.c2,'enable','on');
set(handles.c2,'Value',1);
set(handles.c3,'enable','on');
set(handles.c7,'enable','on');
set(handles.c20,'enable','on');
set(handles.t3,'visible','off');
set(handles.t6,'visible','off');
set(handles.e13,'visible','off');
set(handles.t9,'visible','off');
set(handles.g2,'visible','on');
a=get(handles.c7,'Value');
set(handles.c8,'Value',0);
set(handles.c9,'Value',0);
set(handles.c10,'Value',0);
set(handles.e2,'enable','off');
set(handles.e3,'enable','off');
set(handles.e4,'enable','off');
set(handles.e5,'enable','off');
set(handles.e6,'enable','off');
set(handles.e7,'enable','off');
set(handles.e8,'enable','off');
set(handles.e9,'enable','off');
set(handles.e10,'enable','off');
if a==1
    set(handles.c1,'enable','off')
    set(handles.c6,'enable','off');
    set(handles.c5,'enable','off');
    set(handles.c4,'enable','off');
    set(handles.c2,'enable','off');
    set(handles.c3,'enable','off');
end
if handles.sis==2
    set(handles.g2,'visible','on');
    set(handles.r7,'Enable','on');
    set(handles.r6,'Enable','on');
    set(handles.r8,'Enable','on');
    set(handles.r9,'Enable','on');
    set(handles.r10,'enable','on');
    set(handles.r11,'Enable','on');
    set(handles.t12,'visible','OFF')
    set(handles.t13,'visible','OFF')
    CALCULAR_Callback(hObject, eventdata, handles)

end

% --- Executes on button press in r4.
function r4_Callback(hObject, eventdata, handles)
% hObject    handle to r4 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
set(handles.c8,'enable','on')
set(handles.g2,'visible','on')
set(handles.c9,'enable','on')
set(handles.c10,'enable','on')
set(handles.t12,'visible','on')
set(handles.t13,'visible','on')
set(handles.t5,'visible','off')
set(handles.t4,'visible','off')
set(handles.t1,'visible','off')
set(handles.t2,'visible','off')
set(handles.t7,'visible','off')
set(handles.t8,'visible','off')
set(handles.e11,'visible','off')
set(handles.e12,'visible','off')
set(handles.t10,'visible','off')
set(handles.t11,'visible','off')
set(handles.text39,'visible','off')
set(handles.text40,'visible','off')
set(handles.text42,'visible','off')
set(handles.c1,'enable','on');
set(handles.c6,'enable','on');
set(handles.c5,'enable','on');
set(handles.c5,'String','Elastic Enthalpy (Black dashed)');
set(handles.c4,'enable','on');
set(handles.c2,'enable','on');
set(handles.c3,'enable','on');
set(handles.c7,'enable','on');
set(handles.c20,'enable','on');
set(handles.t3,'visible','off');
set(handles.t6,'visible','off');
set(handles.e13,'visible','off');
set(handles.t9,'visible','off');
a=get(handles.c7,'Value');
if a==1
    set(handles.c1,'enable','off')
    set(handles.c6,'enable','off');
    set(handles.c5,'enable','off');
    set(handles.c4,'enable','off');
    set(handles.c2,'enable','off');
    set(handles.c3,'enable','off');
    set(handles.c20,'enable','off');
end
if handles.sis==2
    set(handles.r7,'Enable','on');
    set(handles.r6,'Enable','on');
    set(handles.r8,'Enable','on');
    set(handles.r9,'Enable','on');
    set(handles.r10,'enable','on');
    set(handles.r11,'Enable','on');
    set(handles.t12,'visible','OFF')
    set(handles.t13,'visible','OFF')
    CALCULAR_Callback(hObject, eventdata, handles)  
    
end

% Hint: get(hObject,'Value') returns toggle state of r4

% --- Executes on button press in r5.
function r5_Callback(hObject, eventdata, handles)
% hObject    handle to r5 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
set(handles.c8,'enable','off')
set(handles.c9,'enable','off')
set(handles.c10,'enable','off')
cla reset
set(handles.t12,'visible','off')
set(handles.t13,'visible','off')
set(handles.g2,'visible','off')
set(handles.t5,'visible','on')
set(handles.t4,'visible','on')
set(handles.t1,'visible','on')
set(handles.t2,'visible','on')
set(handles.t7,'visible','on')
set(handles.t8,'visible','on')
set(handles.e11,'visible','on')
set(handles.e12,'visible','on')
set(handles.t10,'visible','on')
set(handles.t11,'visible','on')
set(handles.text39,'visible','on')
set(handles.text40,'visible','on')
set(handles.text42,'visible','on')
set(handles.c1,'enable','off');
set(handles.c6,'enable','off');
set(handles.c5,'enable','off');
set(handles.c4,'enable','off');
set(handles.c3,'enable','off');
set(handles.c2,'enable','off');
set(handles.c7,'enable','off');
set(handles.c20,'enable','off');
handles.counter=0;


if handles.sis==2
    set(handles.t3,'visible','on');
    set(handles.t6,'visible','on');
    set(handles.e13,'visible','on');
    set(handles.t9,'visible','on');
    set(handles.r7,'Enable','off');
    set(handles.r6,'Enable','off');
    set(handles.r8,'Enable','off');
    set(handles.r9,'Enable','off');
    set(handles.r10,'Enable','off');
    set(handles.r11,'Enable','off');
    set(handles.t14,'Visible','off');
    set(handles.t15,'Visible','off');
    set(handles.t16,'Visible','off');


end


function Li_Callback(hObject, eventdata, handles)
% set(hObject,'Backgroundcolor',[0 1 0]); %Ver como dar dos click para
% cambiar de color 
die=get(hObject,'Value');
handles.die=die;
if handles.sis==1
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.preslitio);
        if handles.contador==2
            set(handles.Li,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unpreslitio);
        elseif handles.contador==1
            handles.b1='Li'
            handles.b2=13;
            handles.b3=2.85;
            handles.b4=0.94;
            handles.b5=1.158e10;
            handles.b6=0.4228e10;
            handles.b7=1;
            handles.b8=0;
            handles.Aw2=6.941;
            handles.gam2=398;       %mN/m
            handles.b9=453.69;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==0
            handles.a1='Li'
            handles.a2=13;
            handles.a3=2.85;
            handles.a4=0.94;
            handles.a5=1.158e10;
            handles.a6=0.4228e10;
            handles.a7=1;
            handles.a8=0;
            handles.Aw1=6.941;
            handles.gam1=398;    %mN/m
            handles.a9=453.69;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;
        
        elseif handles.contador==3
            set(handles.Li,'Value',0)
            set(hObject,'Backgroundcolor', handles.unpreslitio);
            handles.contador=2;
        end
    else
        set(hObject,'Backgroundcolor', handles.unpreslitio);
        if handles.a1=='Li'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;     %K
            set(handles.t4,'String',handles.b1)
 
           handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Li'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.d1=='Li'
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
else
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.preslitio);
        if handles.contador==3
            set(handles.Li,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unpreslitio);
        elseif handles.contador==2
            handles.d1='Li'
            handles.d2=13;
            handles.d3=2.85;
            handles.d4=0.94;
            handles.d5=1.158e10;
            handles.d6=0.4228e10;
            handles.d7=1;
            handles.d8=0;
            handles.Aw3=6.941;
            handles.gam3=398;       %mN/m
            handles.d9=453.69;     %K
            set(handles.t6,'String',handles.d1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='Li'
            handles.b2=13;
            handles.b3=2.85;
            handles.b4=0.94;
            handles.b5=1.158e10;
            handles.b6=0.4228e10;
            handles.b7=1;
            handles.b8=0;
            handles.Aw2=6.941;
            handles.gam2=398;    %mN/m
            handles.b9=453.69;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='Li'
            handles.a2=13;
            handles.a3=2.85;
            handles.a4=0.94;
            handles.a5=1.158e10;
            handles.a6=0.4228e10;
            handles.a7=1;
            handles.a8=0;
            handles.Aw1=6.941;
            handles.gam1=398;    %mN/m
            handles.a9=453.69;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;       
        elseif handles.contador==4
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unpreslitio);
        if handles.a1=='Li'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;
            set(handles.t4,'String',handles.b1)
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
 
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.b1=='Li'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        else
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
end
handles
guidata(hObject, handles);

% --- Executes on button press in Be.
function Be_Callback(hObject, eventdata, handles)
% hObject    handle to Be (see GCBO)
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
die=get(hObject,'Value');
handles.die=die;
if handles.sis==1
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.preslitio);
        if handles.contador==2
            set(handles.Be,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unpreslitio);
        elseif handles.contador==1
            handles.b1='Be'
            handles.b2=4.9;
            handles.b3=5.05;
            handles.b4=4.66;
            handles.b5=10.04e10;
            handles.b6=14.32e10;
            handles.b7=1;
            handles.b8=0.4;
            handles.Aw2=9.01218;
            handles.gam2=1390;    %mN/m
            handles.b9=1560;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==0
            handles.a1='Be'
            handles.a2=4.9;
            handles.a3=5.05;
            handles.a4=4.66;
            handles.a5=10.04e10;
            handles.a6=14.32e10;
            handles.a7=1;        
            handles.a8=0.4;
            handles.Aw1=9.01218;
            handles.gam1=1390;
            handles.a9=1560;     %K
            set(handles.t4,'String',handles.a1);
            handles.contador=handles.contador+1;
        
        elseif handles.contador==3
            set(handles.Be,'Value',0);
            set(hObject,'Backgroundcolor', handles.unpreslitio);
            handles.contador=2;
        end
    else
        set(hObject,'Backgroundcolor', handles.unpreslitio);
        if handles.a1=='Be'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;     %K
            set(handles.t4,'String',handles.b1);
        
           handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1);
            handles.d1=' ';
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ');
            handles.contador=handles.contador-1;
        elseif handles.b1=='Be'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;     %K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.d1=='Be'
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
else
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.preslitio);
        if handles.contador==3
            set(handles.Be,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unpreslitio);
        elseif handles.contador==2
            handles.d1='Be'
            handles.d2=4.9;
            handles.d3=5.05;
            handles.d4=4.66;
            handles.d5=10.04e10;
            handles.d6=14.32e10;
            handles.d7=1;
            handles.d8=0;
            handles.Aw3=6.941;
            handles.gam3=398;       %mN/m
            handles.d9=453.69;     %K
            set(handles.t6,'String',handles.d1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='Be'
            handles.b2=4.9;
            handles.b3=5.05;
            handles.b4=4.66;
            handles.b5=10.04e10;
            handles.b6=14.32e10;
            handles.b7=1;
            handles.b8=0;
            handles.Aw2=6.941;
            handles.gam2=398;    %mN/m
            handles.b9=453.69;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='Be'
            handles.a2=4.9;
            handles.a3=5.05;
            handles.a4=4.66;
            handles.a5=10.04e10;
            handles.a6=14.32e10;
            handles.a7=1;
            handles.a8=0;
            handles.Aw1=6.941;
            handles.gam1=398;    %mN/m
            handles.a9=453.69;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;       
        elseif handles.contador==4
            handles.contador=0
        end
    else
        set(hObject,'Backgroundcolor', handles.unpreslitio);
        if handles.a1=='Be'
            handles.a1=handles.b1;
            handles.a2=handles.b2;
            handles.a3=handles.b3;
            handles.a4=handles.b4;
            handles.a5=handles.b5;
            handles.a6=handles.b6;
            handles.a7=handles.b7;
            handles.a8=handles.b8;
            handles.Aw1=handles.Aw2;
            handles.gam1=handles.gam2;
            handles.a9=handles.b9;
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t4,'String',handles.b1)
            set(handles.t5,'String',handles.d1)
 
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1;
        elseif handles.b1=='Be'
            handles.b1=handles.d1;
            handles.b2=handles.d2;
            handles.b3=handles.d3;
            handles.b4=handles.d4;
            handles.b5=handles.d5;
            handles.b6=handles.d6;
            handles.b7=handles.d7;
            handles.b8=handles.d8;
            handles.Aw2=handles.Aw3;
            handles.gam2=handles.gam3;
            handles.b9=handles.d9;%K
            set(handles.t5,'String',handles.d1)
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        else
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
