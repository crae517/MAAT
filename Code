function N_Callback(hObject, eventdata, handles)
% hObject    handle to N (see GCBO)
% eventdata  reserved - to be defined in t12 future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
die=get(hObject,'Value');
handles.die=die;
if handles.sis==1
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.preslitio);
        if handles.contador==2
            set(handles.N,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unpreslitio);
        elseif handles.contador==1
            handles.b1='N'
            handles.b2=4.1;
            handles.b3=6.86;
            handles.b4=4.49;
            handles.b5=0e10;
            handles.b6=0e10;
            handles.b7=1;
            handles.b8=2.3;
            handles.Aw2=14.00674;
            handles.gam2=0;    %mN/m
            handles.b9=63.05;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==0
            handles.a1='N'
            handles.a2=4.1;
            handles.a3=6.86;
            handles.a4=4.49;
            handles.a5=0e10;
            handles.a6=0e10;
            handles.a7=1;        
            handles.a8=2.3;
            handles.Aw1=14.00674;
            handles.gam1=0;
            handles.a9=63.05;     %K
            set(handles.t4,'String',handles.a1)
            handles.contador=handles.contador+1;
        
        elseif handles.contador==3
            handles.contador=0
        end
    else
