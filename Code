% --- Executes on button press in r1.
function r1_Callback(hObject, eventdata, handles)
% hObject    handle to r1 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
cla reset
set(handles.t3,'visible','off');
set(handles.t6,'visible','off');
set(handles.e13,'visible','off');
set(handles.t9,'visible','off');
set(handles.c1,'Visible','on');
set(handles.c6,'Visible','on');
set(handles.c5,'Visible','on');
set(handles.c4,'Visible','on');
set(handles.c2,'Visible','on');
set(handles.c3,'VIsible','on');
set(handles.c7,'VIsible','on');
set(handles.c20,'VIsible','on');
set(handles.g1,'Visible','off');
set(handles.t14,'Visible','off');
set(handles.t15,'Visible','off');
set(handles.t16,'Visible','off');
set(handles.p1,'Enable','off');
if handles.mode==3
    set(handles.g2,'Visible','off');
end 

if handles.contador==3
    errordlg('Must select two elements','Error');
end



% Hint: get(hObject,'Value') returns toggle state of r1


% --- Executes on button press in r2.
function r2_Callback(hObject, eventdata, handles)
% hObject    handle to r2 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
cla reset
set(handles.c1,'Visible','off');
set(handles.c6,'Visible','off');
set(handles.c5,'Visible','off');
set(handles.c4,'Visible','off');
set(handles.c2,'Visible','off');
set(handles.c3,'VIsible','off');
set(handles.c7,'VIsible','off');
set(handles.c20,'Visible','off');
set(handles.g1,'Visible','on');
set(handles.p1,'Enable','on');
set(handles.r7,'Enable','on');
set(handles.r6,'Enable','on');
set(handles.r8,'Enable','on');
set(handles.r9,'Enable','on');
set(handles.r10,'Enable','on');  
set(handles.r11,'Enable','on');
if handles.mode==3
    set(handles.g2,'Visible','off');
    set(handles.t3,'visible','on');
    set(handles.t6,'visible','on');
    set(handles.e13,'visible','on');
    set(handles.t9,'visible','on');
    set(handles.t14,'Visible','off');
    set(handles.t15,'Visible','off');
    set(handles.t16,'Visible','off');
    set(handles.r7,'Enable','off');
    set(handles.r6,'Enable','off');
    set(handles.r8,'Enable','off');
    set(handles.r9,'Enable','off');
    set(handles.r10,'Enable','off');
    set(handles.r11,'Enable','off');

end
% Hint: get(hObject,'Value') returns toggle state of r2

% --- Executes when selected object is changed in g1.
function g1_SelectionChangedFcn(hObject, eventdata, handles)
% hObject    handle to the selected object in g1 
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
graph=get(hObject,'String');
switch graph
    case'Enthalpy of Mixing'
        handles.graph=1;
    case'Modified Gibbs Free Energy'
        handles.graph=2;
    case'Entropic term'
        handles.graph=3;
    case'Gibbs Free Energy'
        handles.graph=4;
    case'Activity'
        handles.graph=5;
    case'Ideal Gibbs Free Energy '
        handles.graph=6;
end
handles;
guidata(hObject, handles);


