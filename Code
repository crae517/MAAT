            handles.d9=312.46;     %K
            set(handles.t6,'String',handles.d1)
            handles.contador=handles.contador+1
    
        elseif handles.contador==1
            handles.b1='Rb'
            handles.b2=56.07;
            handles.b3=2.1;
            handles.b4=0.22;
            handles.b5=0.3145e10;
            handles.b6=0.1001e10;
            handles.b7=1;
            handles.b8=0;
            handles.Aw2=85.4678;
            handles.gam2=115;    %mN/m
            handles.b9=312.46;     %K
            set(handles.t5,'String',handles.b1)
            handles.contador=handles.contador+1;
        elseif handles.contador==0
            handles.a1='Rb'
            handles.a2=56.07;
            handles.a3=2.1;
            handles.a4=0.22;
            handles.a5=0.3145e10;
