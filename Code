            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        elseif handles.d1=='Rb'
            handles.d1=' '
            handles.d2=0;
            handles.d3=0;
            handles.d4=0;
            handles.d5=0;
            handles.d6=0;
            handles.d7=0;
            handles.d8=0;
            handles.Aw3=0;
            handles.gam3=0;
            handles.d9=0;     %K
            set(handles.t6,'String',' ')
            handles.contador=handles.contador-1
        end
    end
else
    if handles.die==1
        set(hObject,'Backgroundcolor', handles.preslitio);
        if handles.contador==3
            set(handles.Rb,'Value',0)
            errordlg('Must select two elements','Error');
            set(hObject,'Backgroundcolor', handles.unpreslitio);
        elseif handles.contador==2
            handles.d1='Rb'
            handles.d2=56.07;
            handles.d3=2.1;
            handles.d4=0.22;
            handles.d5=0.3145e10;
            handles.d6=0.1001e10;
